/*4- Crie um programa que:
    Possua um vetor de 5 números inteiros
    peça ao usuário para digitar os valores
    exiba todos os valores*/

#include <iostream>
   using namespace std;
   int main() {
       int n[5];
       for (int i = 0; i < 5; i++) {
           cout << "Digite o valor do número ";
       cin >> n[i];}


       cout << "Os valores digitados foram: ";
       for (int i = 0; i < 5; i++) {
           cout << n[i] << " ";
       }
   return 0;
   }
